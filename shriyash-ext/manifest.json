{
  "manifest_version": 2,
  "name": "Shriyash: The Extension",
  "description": "i'm sorry",
  "version": "0.0.1",
  "options_page": "html/config.html",
  "browser_action": {
    "default_icon": {
      "19": "images/shriyash.png"
    },
    "default_title": "shriyash"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ], 
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "images/shriyash.png"
  }
}